<?xml version="1.0" encoding="utf-8"?>
<framework>
    <keyboard>
        <if
            device="tablet"
            device_size="large"
            keyboard_mode="normal">
            <view
                layout="@layout/layout_7f0e0311"
                type="body" />
        </if>
        <else_if
            keyboard_mode="split">
            <if
                split_with_duplicate_keys="true">
                <view
                    layout="@layout/layout_7f0e0286"
                    type="body" />
            </if>
            <else>
                <view
                    layout="@layout/layout_7f0e0287"
                    type="body" />
            </else>
        </else_if>
        <else>
            <view
                layout="@layout/layout_7f0e0285"
                type="body" />
        </else>
        <view
            type="body">
            <softkeys
                href="@xml/xml_7f170fc1" />
            <include
                href="@xml/xml_7f170bcf" />
            <include
                href="@xml/xml_7f170b8e" />
        </view>
    </keyboard>
</framework>