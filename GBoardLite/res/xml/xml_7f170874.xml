        <else>
            <view
                layout="@layout/layout_7f0e023e"
                type="body" />
        </else>
        <view
            type="body">
            <softkeys
                href="@xml/xml_7f170fc1" />
            <include
                href="@xml/xml_7f170bcf" />
            <if
                device="tablet"
                device_size="large"
                keyboard_mode="normal">
                <include
                    href="@xml/xml_7f170b90" />
            </if>
            <else>
                <include
                    href="@xml/xml_7f170b8d" />
            </else>
        </view>
    </keyboard>
</framework>