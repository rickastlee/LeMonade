}

key APOSTROPHE {
    label:                              '\''
    base:                               '\''
    shift:                              '\u0022'
}

key BACKSLASH {
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
}

### ROW 4

key PLUS {
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
}

key Z {
    label:                              'Z'
    base, capslock+shift:               'z'
    shift, capslock:                    'Z'
}

key X {
    label:                              'X'
    base, capslock+shift:               'x'
