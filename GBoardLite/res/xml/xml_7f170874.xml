<?xml version="1.0" encoding="utf-8"?>
<framework>
    <keyboard>
        <if
            keyboard_mode="split">
            <if
                device="tablet"
                device_size="large">
                <if
                    split_with_duplicate_keys="true">
                    <view
                        layout="@layout/layout_7f0e0372"
                        type="body" />
                </if>
                <else>
                    <view
                        layout="@layout/layout_7f0e0373"
                        type="body" />
                </else>
            </if>
            <else_if
                enable_iw_staggered_layout="true">
                <if
                    split_with_duplicate_keys="true">
                    <view
                        layout="@layout/layout_7f0e0372"
                        type="body" />
                </if>
                <else>
                    <view
                        layout="@layout/layout_7f0e0373"
                        type="body" />
                </else>
            </else_if>
            <else>
                <view
                    layout="@layout/layout_7f0e023f"
                    type="body" />
            </else>
        </if>
        <else_if
            device="tablet"
            device_size="large">
            <if
                keyboard_mode="normal">
                <view
                    layout="@layout/layout_7f0e02ce"
                    type="body" />
            </if>
            <else>
                <view
                    layout="@layout/layout_7f0e0371"
