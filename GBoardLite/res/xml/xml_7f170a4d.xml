<?xml version="1.0" encoding="utf-8"?>
<framework>
    <if
        split_keyboard_handedness_ja="right|left">
        <key_mapping
            state="EMOJI_AVAILABLE">
            <mapping
                key_id="@id/id_7f0b123e"
                view_id="@id/key_pos_switch_to_symbol" />
        </key_mapping>
        <if
            expressions="policy_transparency">
            <key_mapping>
                <mapping
                    key_id="@id/id_7f0b1ae8"
                    view_id="@id/key_pos_switch_to_smiley" />
                <mapping
                    key_id="@id/id_7f0b123c"
                    view_id="@id/key_pos_switch_to_emoticon" />
            </key_mapping>
        </if>
        <else_if
            expressions="hidden">
            <key_mapping>
                <mapping
                    key_id="@id/id_7f0b0cba"
                    view_id="@id/key_pos_switch_to_smiley" />
                <mapping
                    key_id="@id/id_7f0b0cba"
                    view_id="@id/key_pos_switch_to_emoticon" />
                <mapping
                    key_id="@id/id_7f0b1240"
                    view_id="@id/key_pos_switch_to_symbol" />
            </key_mapping>
        </else_if>
        <else>
            <key_mapping>
                <mapping
                    key_id="@id/id_7f0b1ae7"
                    view_id="@id/key_pos_switch_to_smiley" />
                <mapping
                    key_id="@id/id_7f0b123b"
                    view_id="@id/key_pos_switch_to_emoticon" />
            </key_mapping>
            <key_mapping
                state="EMOJI_AVAILABLE">
                <mapping
                    key_id="@id/id_7f0b1ae4"
                    view_id="@id/key_pos_switch_to_smiley" />
                <mapping
                    key_id="@id/id_7f0b123a"
                    view_id="@id/key_pos_switch_to_emoticon" />
            </key_mapping>
        </else>
    </if>
</framework>