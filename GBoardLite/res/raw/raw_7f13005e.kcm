    ralt+shift:                         '\u030C'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift:                              '>'
    ralt:                               '\u0323'
}

key SLASH {
    label:                              '/'
    base:                               '/'
    shift:                              '?'
}