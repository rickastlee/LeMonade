# Marathi keyboard layout.

type OVERLAY

map key 86 PLUS

### ROW 1

key GRAVE {
    ralt:                               '`'
    ralt+shift:                         '~'
}

key 1 {
    label:                              '\u0967'
    base:                               '\u0967'
    shift:                              '\u090d'
    ralt:                               '1'
    ralt+shift:                         '!'
    ctrl+shift:                         '\u200d'
}

key 2 {
    label:                              '\u0968'
    base:                               '\u0968'
    shift:                              '\u0945'
