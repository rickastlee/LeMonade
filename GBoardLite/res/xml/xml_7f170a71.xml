<?xml version="1.0" encoding="utf-8"?>
<framework>
    <key_mapping>
        <if
            japanese_use_tri_state_mode="true">
            <mapping
                key_id="@id/id_7f0b1243"
                view_id="@id/key_pos_switch_hiragana_alphabet" />
            <mapping
                key_id="@id/id_7f0b1243"
                view_id="@id/key_pos_switch_to_symbol_right" />
        </if>
        <else>
            <mapping
                key_id="@id/id_7f0b1af0"
                view_id="@id/key_pos_switch_hiragana_alphabet" />
            <mapping
                key_id="@id/id_7f0b1af0"
                view_id="@id/key_pos_switch_to_symbol_right" />
        </else>
        <mapping
            key_id="@id/id_7f0b1240"
            view_id="@id/key_pos_switch_to_symbol" />
        <mapping
            key_id="@id/id_7f0b0a30"
            view_id="@id/key_pos_bottom_symbol_1" />
        <mapping
            key_id="@id/id_7f0b1a97"
            view_id="@id/key_pos_space" />
        <mapping
            key_id="@id/id_7f0b199e"
            view_id="@id/key_pos_bottom_symbol_2" />
        <mapping
            key_id="@id/id_7f0b122a"
            view_id="@id/key_pos_qwerty_globe" />
    </key_mapping>
    <if
        japanese_use_tri_state_mode="true">
        <key_mapping
            state="EMOJI_AVAILABLE">
            <if
                expressions="policy_transparency">
                <mapping
                    key_id="@id/id_7f0b1ae8"
                    view_id="@id/key_pos_switch_to_symbol" />
            </if>
            <else_if
                expressions="hidden">
                <mapping
                    key_id="@id/id_7f0b0cba"
                    view_id="@id/key_pos_switch_to_smiley" />
            </else_if>
            <else>
                <mapping
                    key_id="@id/id_7f0b1ae4"
                    view_id="@id/key_pos_switch_to_symbol" />
            </else>
        </key_mapping>
    </if>
    <key_mapping
        state="INPUT_TYPE_URI,INPUT_TYPE_EMAIL_ADDRESS">
        <mapping
            key_id="@id/id_7f0b1a8d"
            view_id="@id/key_pos_bottom_symbol_1" />
        <if
            device="tablet"
            device_size="large"
            keyboard_mode="normal">
            <mapping
                key_id="@id/id_7f0b0878"
                view_id="@id/key_pos_switch_to_symbol" />
        </if>
        <else_if
            device="tablet"
            enable_show_foldable_layout_on_small_tablet="false"
            keyboard_mode="normal">
            <mapping
                key_id="@id/id_7f0b0878"
                view_id="@id/key_pos_switch_to_symbol" />
        </else_if>
    </key_mapping>
</framework>